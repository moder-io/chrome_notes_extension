{
  "manifest_version": 2,
  "name": "Notas Rápidas",
  "version": "1.0",
  "description": "Una extensión para tomar notas rápidas mientras navegas.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage"]
}
